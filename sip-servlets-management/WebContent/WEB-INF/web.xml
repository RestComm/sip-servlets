<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.2//EN" "http://java.sun.com/j2ee/dtds/web-app_2_2.dtd">
<web-app id="WebApp_ID">
	<display-name>sip-servlets-management</display-name>
	<servlet>
		<servlet-name>DARConfigurationService</servlet-name>
		<servlet-class>
		org.mobicents.servlet.management.server.router.DARConfigurationServiceImpl</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>DARConfigurationService</servlet-name>
		<url-pattern>/DARConfigurationService</url-pattern>
	</servlet-mapping>
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
		<welcome-file>index.htm</welcome-file>
		<welcome-file>index.jsp</welcome-file>
		<welcome-file>default.html</welcome-file>
		<welcome-file>default.htm</welcome-file>
		<welcome-file>default.jsp</welcome-file>
	</welcome-file-list>
</web-app>
